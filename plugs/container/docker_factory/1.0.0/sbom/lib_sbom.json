{
  "docker_factory_plugin": {
    "name": "docker_factory_plugin",
    "version": "1.0.0",
    "owner": "PlugPipe Core Team",
    "type": "factory",
    "category": "container",
    "dependencies": {
      "system": [
        {
          "name": "docker",
          "version": "latest",
          "type": "system",
          "required": true,
          "description": "Container orchestration platform"
        }
      ],
      "python_builtin": [
        {
          "name": "asyncio",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "Asynchronous I/O framework"
        },
        {
          "name": "subprocess",
          "version": "builtin", 
          "type": "library",
          "required": true,
          "description": "Process management utilities"
        },
        {
          "name": "json",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "JSON serialization library"
        },
        {
          "name": "logging",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "Python logging framework"
        },
        {
          "name": "uuid",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "UUID generation library"
        },
        {
          "name": "tempfile",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "Temporary file management"
        },
        {
          "name": "yaml",
          "version": "builtin",
          "type": "library",
          "required": true,
          "description": "YAML parsing library"
        }
      ]
    },
    "capabilities": [
      "containerized_plugin_execution",
      "container_isolation", 
      "resource_management",
      "auto_scaling",
      "container_failover",
      "security_sandboxing",
      "enterprise_orchestration"
    ],
    "security_features": [
      "container_isolation",
      "resource_limits",
      "security_options",
      "network_isolation",
      "read_only_filesystem",
      "no_privilege_escalation"
    ],
    "enterprise_features": [
      "production_ready",
      "kubernetes_native",
      "high_availability",
      "zero_downtime_scaling",
      "comprehensive_monitoring",
      "failover_support"
    ]
  }
}